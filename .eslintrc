{
  "root": true,
  "extends": "airbnb",
  "rules": {
    "comma-dangle": [2, "never"],
    "no-shadow": [0],
    "spaced-comment": [0],
    "no-console": [0],
    "consistent-return": [0]
  },
  "ecmaFeatures": {
    "experimentalObjectRestSpread": [0]
  },
  "env": {
    "browser": true
  },
  "globals": {
    "io": false,
    "RTCPeerConnection": false,
    "webrtcUtils": false,
    "getUserMedia": false,
    "attachMediaStream": false
  }
}
